{
  "elementTypes": [
    "ConcreteFeature",
    "RootFeature",
    "AbstractFeature"
  ],
  "elementTranslationRules": {
    "RootFeature": {
      "param": "F",
      "constraint": "(and (bool UUID_F) (= UUID_F 1))",
      "selectedConstraint": "(and (bool UUID_F) (= UUID_F 1))",
      "deselectedConstraint": "(= 0 1)"
    },
    "AbstractFeature": {
      "param": "F",
      "constraint": "(bool UUID_F)",
      "selectedConstraint": "(and (bool UUID_F) (= UUID_F 1))",
      "deselectedConstraint": "(and (bool UUID_F) (= UUID_F 0))"
    },
    "ConcreteFeature": {
      "param": "F",
      "constraint": "(bool UUID_F)",
      "selectedConstraint": "(and (bool UUID_F) (= UUID_F 1))",
      "deselectedConstraint": "(and (bool UUID_F) (= UUID_F 0))"
    }
  },
  "attributeTypes": [
    "RootFeature",
    "AbstractFeature",
    "ConcreteFeature"
  ],
  "attributeTranslationRules": {
    "Integer":{
      "parent": "F",
      "param": "id",
      "template": "A",
      "constraint": "(int UUID_A)"
    },
    "String":{
      "parent":"F",
      "param": "id",
      "template": "A",
      "values": "Xs",
      "constraint": "(enum (Xs) UUID_A)"
    },
    "Boolean":{
      "parent":"F",
      "param": "id",
      "template": "A",
      "constraint":"(bool UUID_A)"
    }
  },
  "typingRelationTypes": [
    "IndividualCardinality"
  ],
  "typingRelationTranslationRules": {
    "IndividualCardinality": {
      "derivingRelationInbound": true,
      "param": [
        "F1",
        "F2",
        "min",
        "max"
      ],
      "relationLookupSchema": {
        "min": {
          "index": 1,
          "key": "value"
        },
        "max": {
          "index": 2,
          "key": "value"
        }
      },
      "constraint": "(and (int (0 max) UUID_F1) (and (=< (UUID_F2 * min) UUID_F1) (=< UUID_F1 (UUID_F2 * max)) ))"
    }
  },
  "hierarchyTypes":[],
  "hierarchyTranslationRules":{},
  "relationTypes": [
    "Mandatory",
    "Optional",
    "Includes",
    "Excludes",
    "Range",
    "Xor",
    "And",
    "Or"
  ],
  "relationReificationTypes": [
    "Bundle"
  ],
  "relationReificationTranslationRules":{
    "Bundle": {
      "param": [
        "F",
        "Xs",
        "min",
        "max"
      ],
      "paramMapping": {
        "inboundEdges": {
          "unique": true,
          "var": "F"
        },
        "outboundEdges": {
          "unique": false,
          "var": "Xs"
        }
      },
      "constraint": {
        "Or": "(and (=< F (sum(Xs))) (=< (sum(Xs)) (F * len(Xs))) )",
        "And": "(= (sum(Xs)) (F * len(Xs)))",
        "Xor": "(= (sum(Xs)) F )",
        "Range": "(and (=< (F * min) (sum(Xs))) (=< (sum(Xs)) (F * max)) )"
      }
    }
  },
  "relationReificationTypeDependentExpansions": {
    "Bundle": {
      "Range": [
        "max",
        "min"
      ]
    }
  },
  "relationReificationPropertySchema": {
    "type": {
      "index": 1,
      "key": "value"
    },
    "min": {
      "index": 2,
      "key": "value"
    },
    "max": {
      "index": 3,
      "key": "value"
    }
  },
  "relationReificationExpansions": {
    "params": [
      "Xs"
    ],
    "functions": [
      "sum",
      "len"
    ]
  },
  "relationPropertySchema": {
    "type": {
      "index": 0,
      "key": "value"
    }
  },
  "relationTranslationRules": {
    "Excludes": {
      "params": [
        "F1",
        "F2"
      ],
      "constraint": "(=< (UUID_F1 + UUID_F2) 1)"
    },
    "Includes": {
      "params": [
        "F1",
        "F2"
      ],
      "constraint": "(>= UUID_F2 UUID_F1)"
    },
    "Optional": {
      "params": [
        "F1",
        "F2"
      ],
      "constraint": "(>= UUID_F1 UUID_F2)"
    },
    "Mandatory": {
      "params": [
        "F1",
        "F2"
      ],
      "constraint": "(= UUID_F1 UUID_F2)"
    }
  }
}